#################################################################
#    Key Testimonies (Sample from each bill)
#################################################################

# HB767_SD1 Testimonies
:testimony_HB767_DOE rdf:type owl:NamedIndividual ,
                            :Testimony ;
                    :provides_testimony :Department_of_Education_Testimony ;
                    :testifies_on :HB767_SD1 ;
                    :has_position :Support ;
                    :hasComments "Supports moving the program to DOE, wants baseline assessment, requests dedicated funding for coordinator position. Requests resources for managing new goals." ;
                    :hasSourceFile "HB767_SD1_TESTIMONY_WAM_04-01-21.pdf" .

:testimony_HB767_Local_Food_Coalition rdf:type owl:NamedIndividual ,
                                             :Testimony ;
                                     :provides_testimony :Local_Food_Coalition ;
                                     :testifies_on :HB767_SD1 ;
                                     :has_position :Support ;
                                     :hasComments "Supports 30% local sourcing goal with annual reporting; notes only 3% is local now. Recommends geographic preference, food hubs, localized strategy." ;
                                     :hasSourceFile "HB767_SD1_TESTIMONY_WAM_04-01-21.pdf" .

# SB2182_HD1 Testimonies
:testimony_SB2182_DOH rdf:type owl:NamedIndividual ,
                             :Testimony ;
                     :provides_testimony :Department_of_Health ;
                     :testifies_on :SB2182_HD1 ;
                     :has_position :Support ;
                     :hasComments "Supports establishing a school garden coordinator, details health and academic benefits of gardens, notes hands-on learning increases consumption." ;
                     :hasSourceFile "SB2182_HD1_TESTIMONY_FIN_04-05-22.pdf" .

:testimony_SB2182_Farm_to_School_Hui rdf:type owl:NamedIndividual ,
                                             :Testimony ;
                                     :provides_testimony :Hawaii_Farm_to_School_Hui_SB2182 ;
                                     :testifies_on :SB2182_HD1 ;
                                     :has_position :Strong_Support ;
                                     :hasComments "Details garden and farm-based education benefits, requests committee to pass bill, outlines statewide network's priorities." ;
                                     :hasSourceFile "SB2182_HD1_TESTIMONY_FIN_04-05-22.pdf" .

# SB666 Testimonies
:testimony_SB666_CTAHR rdf:type owl:NamedIndividual ,
                              :Testimony ;
                      :provides_testimony :UH_Manoa_CTAHR ;
                      :testifies_on :SB666 ;
                      :has_position :Support_with_Amendment ;
                      :hasComments "Requests correction: position is on Moloka'i not Kaua'i. Supports coordinator and permanent extension agent positions as workforce investments." ;
                      :hasSourceFile "SB666_TESTIMONY_HRE_01-30-25.pdf" .

# SB1548 Testimonies
:testimony_SB1548_Ulupono rdf:type owl:NamedIndividual ,
                                  :Testimony ;
                          :provides_testimony :Ulupono_Initiative_SB1548 ;
                          :testifies_on :SB1548 ;
                          :has_position :Support ;
                          :hasComments "Supports procurement reforms, notes DOE's buying power, need for supply chain capacity, and farm-to-school goals (30% by 2030)." ;
                          :hasSourceFile "SB1548_TESTIMONY_EDU_02-07-25.pdf" .

# HB1293 Testimonies
:testimony_HB1293_PHI rdf:type owl:NamedIndividual ,
                             :Testimony ;
                     :provides_testimony :Hawaii_Public_Health_Institute_HB1293 ;
                     :testifies_on :HB1293 ;
                     :has_position :Strong_Support ;
                     :hasComments "Promotes local procurement, references regional model for learning and accountability recommendations, supports streamlined process for local foods." ;
                     :hasSourceFile "HB1293_TESTIMONY_EDN_02-11-25_.PDF.pdf" .

