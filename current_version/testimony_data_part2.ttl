# SB1548 Testimonies
:testimony_SB1548_Noel_Shaw rdf:type owl:NamedIndividual ,
                                   :Testimony ;
                           :provides_testimony :Noel_Shaw ;
                           :testifies_on :SB1548 ;
                           :has_position :Support ;
                           :hasComments "Advocates for increasing DOE buying power to match other departments, investment in food systems and ag sector." ;
                           :has_source "SB1548_TESTIMONY_EDU_02-07-25.pdf" .

:testimony_SB1548_Farm_Link rdf:type owl:NamedIndividual ,
                                   :Testimony ;
                           :provides_testimony :Farm_Link_Hawaii ;
                           :testifies_on :SB1548 ;
                           :has_position :Support ;
                           :hasComments "Supports increasing small purchase threshold, notes benefits for schools, farmers, and procurement ease for local food goals." ;
                           :has_source "SB1548_TESTIMONY_EDU_02-07-25.pdf" .

:testimony_SB1548_Ulupono rdf:type owl:NamedIndividual ,
                                  :Testimony ;
                          :provides_testimony :Ulupono_Initiative_SB1548 ;
                          :testifies_on :SB1548 ;
                          :has_position :Support ;
                          :hasComments "Supports procurement reforms, notes DOE's buying power, need for supply chain capacity, and farm-to-school goals (30% by 2030)." ;
                          :has_source "SB1548_TESTIMONY_EDU_02-07-25.pdf" .

:testimony_SB1548_Jessica_Sobocinski rdf:type owl:NamedIndividual ,
                                            :Testimony ;
                                    :provides_testimony :Jessica_Sobocinski ;
                                    :testifies_on :SB1548 ;
                                    :has_position :Support ;
                                    :hasComments "States procurement barriers for local food, advocates raising the threshold to facilitate access for producers, supports 30% local by 2030 goal." ;
                                    :has_source "SB1548_TESTIMONY_EDU_02-07-25.pdf" .

:testimony_SB1548_Debbie_Millikan rdf:type owl:NamedIndividual ,
                                         :Testimony ;
                                 :provides_testimony :Debbie_Millikan ;
                                 :testifies_on :SB1548 ;
                                 :has_position :Support ;
                                 :hasComments "Calls for threshold raise, links to 30% local goal, stresses improved student access and support for local farmers." ;
                                 :has_source "SB1548_TESTIMONY_EDU_02-07-25.pdf" .

:testimony_SB1548_Procurement_Office rdf:type owl:NamedIndividual ,
                                            :Testimony ;
                                    :provides_testimony :State_Procurement_Office ;
                                    :testifies_on :SB1548 ;
                                    :has_position :Recommend_Amendments ;
                                    :hasComments "Suggests specific procurement language and warns against parceling; supports increased procurement flexibility." ;
                                    :has_source "SB1548_TESTIMONY_EDU_02-07-25.pdf" .

:testimony_SB1548_DOE rdf:type owl:NamedIndividual ,
                              :Testimony ;
                      :provides_testimony :Department_of_Education_SB1548 ;
                      :testifies_on :SB1548 ;
                      :has_position :Support ;
                      :hasComments "Supports threshold increase, stresses effect on local food purchasing, recommends specific amounts, supports written justification for vendor shortages." ;
                      :has_source "SB1548_TESTIMONY_EDU_02-07-25.pdf" .

:testimony_SB1548_Jeannette_Soon_Ludes rdf:type owl:NamedIndividual ,
                                               :Testimony ;
                                       :provides_testimony :Jeannette_Soon_Ludes ;
                                       :testifies_on :SB1548 ;
                                       :has_position :Support ;
                                       :hasComments "Notes current $25,000 limit is a barrier, calls for $100,000 threshold for easier procurement, supports local food access for students." ;
                                       :has_source "SB1548_TESTIMONY_EDU_02-07-25.pdf" .

