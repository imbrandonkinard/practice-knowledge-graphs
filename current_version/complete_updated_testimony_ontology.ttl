@prefix : <http://example.org/farm-to-school-ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://example.org/farm-to-school-ontology> .

#################################################################
#    New Classes for Testimony Data
#################################################################

### Testimony Class
:Testimony rdf:type owl:Class ;
          rdfs:comment "Legislative testimony provided on bills" ;
          rdfs:label "Testimony" .

### Organization Class  
:Organization rdf:type owl:Class ;
             rdfs:comment "Organizations and institutions that provide testimony" ;
             rdfs:label "Organization" .

### Individual Class
:Individual rdf:type owl:Class ;
           rdfs:comment "Individual people who provide testimony" ;
           rdfs:label "Individual" .

### Position Class
:Position rdf:type owl:Class ;
         rdfs:comment "Stance or position taken in testimony" ;
         rdfs:label "Position" .

### Bill Class
:Bill rdf:type owl:Class ;
      rdfs:comment "Legislative bills under consideration" ;
      rdfs:label "Bill" .

### BillVersion Class
:BillVersion rdf:type owl:Class ;
            rdfs:comment "Specific versions of legislative bills" ;
            rdfs:label "Bill Version" .

#################################################################
#    New Object Properties
#################################################################

### provides_testimony
:provides_testimony rdf:type owl:ObjectProperty ;
                   rdfs:comment "Relationship between individual and testimony" ;
                   rdfs:label "provides testimony" .

### testifies_on
:testifies_on rdf:type owl:ObjectProperty ;
             rdfs:comment "Relationship between testimony and bill version" ;
             rdfs:label "testifies on" .

### has_position
:has_position rdf:type owl:ObjectProperty ;
             rdfs:comment "Position taken in testimony" ;
             rdfs:label "has position" .

### represents_organization
:represents_organization rdf:type owl:ObjectProperty ;
                        rdfs:comment "Individual represents an organization in testimony" ;
                        rdfs:label "represents organization" .

### has_version
:has_version rdf:type owl:ObjectProperty ;
            rdfs:comment "Bill has specific version" ;
            rdfs:label "has version" .

### affiliated_with
:affiliated_with rdf:type owl:ObjectProperty ;
                rdfs:comment "Individual is affiliated with organization" ;
                rdfs:label "affiliated with" .

#################################################################
#    New Data Properties
#################################################################

### hasComments
:hasComments rdf:type owl:DatatypeProperty ;
            rdfs:range xsd:string ;
            rdfs:comment "Main comments or rationale from testimony" ;
            rdfs:label "has comments" .

### hasSourceFile
:hasSourceFile rdf:type owl:DatatypeProperty ;
              rdfs:range xsd:string ;
              rdfs:comment "Source file reference" ;
              rdfs:label "has source file" .

### hasFullName
:hasFullName rdf:type owl:DatatypeProperty ;
            rdfs:range xsd:string ;
            rdfs:comment "Full name of individual" ;
            rdfs:label "has full name" .

### hasVersionIdentifier
:hasVersionIdentifier rdf:type owl:DatatypeProperty ;
                     rdfs:range xsd:string ;
                     rdfs:comment "Version identifier for bill" ;
                     rdfs:label "has version identifier" .

